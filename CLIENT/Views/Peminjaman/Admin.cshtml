@{
    ViewData["Title"] = "List Peminjaman";
    Layout = "_AdminTemplate";
}

<div class="card">
    <div class="card-body">
        <h1>Riwayat Peminjaman</h1>

        @*table*@
        <table class="table" id="table_peminjaman">
            <thead>
                <tr>
                    <th>No</th>
                    <th>
                        Aset
                    </th>
                    <th>
                        Peminjam
                    </th>
                    <th>
                        Tanggal Pinjam
                    </th>
                    <th>
                        Tanggal Kembali
                    </th>
                    <th>
                        Jumlah
                    </th>
                    <th>
                        Status
                    </th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>



@section Scripts {
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.12.1/js/jquery.dataTables.js"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="~/js/peminjaman/admin.js"></script>
}
